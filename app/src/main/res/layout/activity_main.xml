<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Main">

    <Button
            android:text="@string/main_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_Login" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="80dp"
            android:layout_marginTop="9dp" app:layout_constraintTop_toBottomOf="@+id/Login_PW"/>
    <Button
            android:text="@string/main_join"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_Join" android:layout_marginStart="55dp"
            app:layout_constraintStart_toEndOf="@+id/btn_Login" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="102dp"
            app:layout_constraintHorizontal_bias="0.379" android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/Login_PW"/>
    <TextView
            android:text="HEALTH"
            android:layout_width="345dp"
            android:layout_height="47dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="16dp"
            android:layout_marginStart="16dp" android:id="@+id/textView13" android:textSize="30sp"
            android:typeface="normal" android:fontFamily="casual" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="23dp" android:textAlignment="center"/>
    <TextView
            android:text="ID"
            android:layout_width="31dp"
            android:layout_height="34dp"
            android:id="@+id/textView14" android:textSize="25sp"
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="PW"
            android:layout_width="46dp"
            android:layout_height="34dp"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView15" android:textSize="25sp" android:layout_marginStart="16dp"
            android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/textView14"/>
    <EditText
            android:layout_width="258dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/Login_ID" android:layout_marginStart="24dp"
            app:layout_constraintStart_toEndOf="@+id/textView14" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="55dp"
            android:privateImeOptions="defaultInputmode=english"
            android:maxLength="14"
            android:digits="abcdefghijklmnopqrstuvwxyz1234567890-_"/>
    <EditText
            android:layout_width="258dp"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/Login_PW"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/Login_ID" app:layout_constraintStart_toEndOf="@+id/textView15"
            android:layout_marginStart="9dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="55dp"
            android:privateImeOptions="defaultInputmode=english;"
            android:maxLength="19"
            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890&amp;&lt;&gt;&apos;&quot;`~!@#$%^*()-_=+{}[]:;,.?/\|"/>
    <TextView
            android:layout_width="357dp"
            android:layout_height="26dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="11dp"
            android:id="@+id/LoginErr" android:layout_marginTop="76dp"
            app:layout_constraintTop_toBottomOf="@+id/Login_PW" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="11dp"/>
</android.support.constraint.ConstraintLayout>